  "license": "MIT",
  "scripts": {
    "start:client:app": "nx run client-app:serve",
    "start:client:api": "nx run client-api:serve",
    "start:reservation:worker": "nx run reservation-worker:serve",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "run:code:generate": "nx run generated:generate",
    "typeorm:generate-migration": "NODE_ENV=local npm run typeorm -- -d ./data-source.ts migration:generate ./migrations/migration",
    "typeorm:generate-create": "NODE_ENV=local npm run typeorm  migration:create  ./migrations/migration",
    "typeorm:run-migrations": "NODE_ENV=local npm run typeorm migration:run -- -d ./data-source.ts",
    "typeorm:revert-migration": "NODE_ENV=local npm run typeorm  -- -d ./data-source.ts migration:revert"
  },
  "private": true,
