    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
